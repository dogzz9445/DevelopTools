<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fileopener="clr-namespace:DDT.Core.WidgetSystems.WPF.Widgets.FileOpeners"
                    xmlns:commanders="clr-namespace:DDT.Core.WidgetSystems.WPF.Widgets.Commanders">
    
    <DataTemplate DataType="{x:Type fileopener:FileOpenerWidgetViewModel}">
        <fileopener:FileOpenerWidget />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type commanders:CommanderWidgetViewModel}">
        <commanders:CommanderWidget />
    </DataTemplate>
    
</ResourceDictionary>